# Etherscan API Key (https://etherscan.io/apis)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Twitter Bearer Token (https://developer.twitter.com/)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# GitHub Personal Access Token (https://github.com/settings/tokens)
GITHUB_TOKEN=your_github_token_here

# Mossland API URL (optional, defaults to https://disclosure.moss.land)
MOSSLAND_API_URL=https://disclosure.moss.land

# Server Port
PORT=4000

# Auto signal collection interval in seconds (0 to disable, default: 60)
SIGNAL_COLLECT_INTERVAL=60
